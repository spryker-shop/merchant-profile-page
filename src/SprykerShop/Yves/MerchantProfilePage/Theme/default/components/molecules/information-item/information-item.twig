{% extends model('component') %}

{% define config = {
    name: 'information-item',
} %}

{% define data = {
    title: required,
    text: '',
} %}

{%- block class -%}spacing-bottom spacing-bottom--big{%- endblock -%}

{% block body %}
    {% block title %}
        <strong>{{ data.title }}</strong>
    {% endblock %}
    {% block text %}
        <div>{{ data.text }}</div>
    {% endblock %}
{% endblock %}
